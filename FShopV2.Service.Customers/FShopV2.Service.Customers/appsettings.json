{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "app": {
    "name": "FShopV2.Service.Customers"
  },
  "mongo": {
    "connectionString": "mongodb://admin:admin@localhost:27017",
    "database": "FShopV2-Customer-Service",
    "seed": false
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "FShopV2.Service.Customers",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const"
  },
  "restEase": {
    "loadBalancer": "",
    "services": [
      {
        "name": "FShopV2.Service.Order",
        "host": "localhost",
        "scheme": "http",
        "port": "5002"
      }
    ]
  },
  "rabbitMq": {
    "namespace": "FShopV2.Service.Customers",
    "retries": 3,
    "retryInterval": 2,
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "durable": true,
      "autoDelete": false,
      "type": "Topic"
    },
    "queue": {
      "autoDelete": false,
      "durable": true,
      "exclusive": false
    }
  }
}
