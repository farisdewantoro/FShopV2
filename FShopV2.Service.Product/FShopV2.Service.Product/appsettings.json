{

  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "app": {
    "name": "FShopV2.Product.Service"
  },
  "mongo": {
    "connectionString": "mongodb://admin:admin@localhost:27017",
    "database": "FShopV2-Product-Service",
    "seed": false
  },
  "rabbitMq": {
